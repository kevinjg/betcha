{
  "name": "agent-editor",
  "version": "1.0.0",
  "description": "VoiceBox Agent Editor",
  "scripts": {
    "build": "npm-run-all clean:dist set-typings --parallel build:*",
    "build:client": "webpack --config config/webpack.prod.js --progress --profile",
    "clean": "npm-run-all --parallel clean:*",
    "clean:dist": "rimraf dist",
    "clean:node_modules": "rimraf node_modules",
    "clean:typings": "rimraf typings",
    "nodemon": "nodemon --delay 4 dist/server/app",
    "postinstall": "typings install",
    "set-typings": "npm-run-all --parallel set-typings:*",
    "set-typings:client": "tsconfig",
    "start": "npm run webpack-dev-server",
    "test:client": "npm-run-all test:client:*",
    "test:client:e2e": "protractor",
    "test:client:unit": "karma start",
    "test:open-report": "open coverage/typescript/index.html",
    "test:remap-coverage": "remap-istanbul -i coverage/coverage.json -o coverage/typescript -t html",
    "update-webdriver": "node_modules/protractor/bin/webdriver-manager update",
    "watch:typings": "onchange 'src/**/*.ts' -- npm run --silent set-typings",
    "webpack-dev-server": "webpack-dev-server --inline --progress --port 8080"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VoiceBoxTechnologies/agent-editor.git"
  },
  "author": "DEVX",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "^2.0.0-rc.6",
    "@angular/compiler": "^2.0.0-rc.6",
    "@angular/core": "^2.0.0-rc.6",
    "@angular/forms": "^2.0.0-rc.6",
    "@angular/http": "^2.0.0-rc.6",
    "@angular/platform-browser": "^2.0.0-rc.6",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.6",
    "@angular/router": "^3.0.0-rc.2",
    "angular2-in-memory-web-api": "0.0.18",
    "angular2-jwt": "^0.1.22",
    "angular2-uuid": "^1.1.0",
    "auth0-lock": "^10.2.2",
    "body-parser": "1.15.2",
    "bootstrap": "3.3.7",
    "core-js": "^2.4.0",
    "cors": "2.7.1",
    "es6-shim": "0.35.0",
    "express": "4.14.0",
    "lodash": "^4.13.1",
    "ng2-bootstrap": "^1.1.1",
    "ng2-toastr": "^0.4.3",
    "pg": "6.1.0",
    "pg-hstore": "2.3.2",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.6",
    "sequelize": "3.24.1",
    "socket.io-client": "^1.4.8",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "chai": "^3.4.0",
    "chai-as-promised": "^5.1.0",
    "codelyzer": "0.0.24",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "istanbul": "^0.4.4",
    "jasmine-core": "^2.4.1",
    "jasmine-spec-reporter": "^2.5.0",
    "karma": "^0.13.22",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "2.3.3",
    "node-sass": "^3.8.0",
    "npm-run-all": "^2.3.0",
    "null-loader": "^0.1.1",
    "onchange": "^2.5.0",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "^3.3.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^4.0.0",
    "sinon": "^1.17.2",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.4",
    "ts-loader": "^0.8.1",
    "ts-node": "^0.7.3",
    "tsconfig-glob": "^0.4.3",
    "tslint": "^3.12.1",
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "webpack": "^2.1.0-beta.22",
    "webpack-dev-server": "^2.1.0-beta.2",
    "webpack-merge": "^0.14.0"
  },
  "bugs": {
    "url": "https://github.com/VoiceBoxTechnologies/agent-editor/issues"
  },
  "homepage": "https://github.com/VoiceBoxTechnologies/agent-editor#readme"
}
